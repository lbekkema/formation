{
  "name": "formation",
  "version": "2.0.0",
  "description": "macOS setup script for front-end developmen",
  "homepage": "https://slay.sh",
  "scripts": {
    "eleventy:serve": "cross-env ELEVENTY_ENV=development eleventy --serve --quiet",
    "eleventy:build": "cross-env ELEVENTY_ENV=production eleventy",
    "sass:test": "sass src/assets/styles:src/assets/styles",
    "sass:watch": "sass src/assets/styles:src/assets/styles --watch --color",
    "sass:build": "sass src/assets/styles:src/assets/styles --no-source-map --style=compressed",
    "dev": "run-p -l sass:build sass:watch eleventy:serve",
    "build": "run-s -l eleventy:build sass:build",
    "start": "npm run dev",
    "clean": "rimraf www .seven11ty.cache",
    "uninstall": "rimraf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/minamarkham/formation.git"
  },
  "bugs": {
    "url": "https://github.com/minamarkham/formation/issues"
  },
  "keywords": ["eleventy", "11ty", "starter", "project", "rss", "markdown", "sitemap"],
  "author": {
    "name": "Mina Markham",
    "email": "mina@mina.codes",
    "url": "https://mina.codes"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.13",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "clean-css": "^5.2.0",
    "cross-env": "^7.0.3",
    "eleventy-critical-css": "^1.0.0",
    "eleventy-plugin-metagen": "^1.4.8",
    "eleventy-plugin-rollup": "^1.0.3",
    "html-minifier": "^4.0.0",
    "js-beautify": "^1.14.0",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.69.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.45.0",
    "uglify-js": "^3.14.2"
  },
  "_moduleAliases": {
    "@711ty": ".seven11ty.js"
  }
}
